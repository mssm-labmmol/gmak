continuation          =  yes
constraints           =  all-bonds
constraint-algorithm  =  lincs
integrator            =  md
dt                    =  0.002
nsteps                =  250000
nstcomm               =  1
;
; Output control.
;
nstxout         = 250
nstvout         = 0
nstenergy       = 250
nstxtcout       = 250    
;
nstlist        =  5
ns_type        =  grid
cutoff-scheme  =  Verlet
rlist          =  1.2
rcoulomb       =  1.2
rvdw           =  1.2
coulombtype    =  cut-off
vdwtype        =  PME
pbc            =  xyz
;
; Berendsen temperature coupling is on
;
Tcoupl   =  v_rescale
tau_t    =  0.10
tc_grps  =  system
ref_t    =  184.52
; Isotropic pressure coupling is now on
Pcoupl           =  Berendsen
Pcoupltype       =  isotropic
tau_p            =  0.5
compressibility  =  4.5e-5
ref_p            =  1.0
gen_vel          =  no
